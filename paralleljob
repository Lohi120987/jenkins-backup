pipeline {
    agent none
    stages{
        stage('build') {
            agent { label 'defaultlabel' }
            steps {
                echo " this is build stage "
                sh " sleep 5 "
         }
    }
        stage('deploy') {
        parallel {
          stage('deploy1') {
            agent { label 'cproject' }
            steps {
                echo " this is deploy stage "
                sh " sleep 5 "
        }
    }
          stage('deploy1') {
            agent { label 'defaultlabel' }
            steps {
                echo " this is deploy stage "
                sh " sleep 5 "
        } 
      }
   }
}
        stage('test') {
            agent any
            steps {
                echo " this is test stage "
                sh " sleep 5 "
            }
          }
    }
}
